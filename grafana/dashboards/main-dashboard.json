      "title": "API Request Rate (5m)",
      "type": "graph",
      "datasource": "prometheus_ds",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"go-api\"}[5m])) by (code)"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "title": "API Error Logs",
      "type": "logs",
      "datasource": "loki_ds",
      "targets": [
        {
          "expr": "{job=\"go-api\"} |= `level=error`"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "title": "Recent API Traces",
